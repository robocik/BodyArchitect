	</Style>
	<Style x:Key="BarOrColumnSeriesBaseStyle" TargetType="charts:BarOrColumnSeriesBase" BasedOn="{StaticResource ChartSeriesBaseStyle}">
		<Setter Property="PointFill" Value="{StaticResource DefaultNonPaletteColor}" />
		<Setter Property="PointStrokeThickness" Value="{StaticResource DefaultBarOrColumnThickness}" />
		<Setter Property="LabelStyle" Value="{StaticResource DefaultDataLabelStyle}" />
		<Setter Property="LegendItemTemplate">
			<Setter.Value>
				<ControlTemplate TargetType="charts:LegendItem">
					<StackPanel Orientation="Horizontal" HorizontalAlignment="Left">
						<Canvas Width="10" Height="10" Margin="0 0 9 0" VerticalAlignment="Center" Opacity="{Binding Opacity}">
							<Path Data="M 0,0 L 10,0 L 10,10 L 0,10 Z" Fill="{Binding PointFill}" Stroke="{Binding PointStroke}" Style="{Binding Path=PointStyle}" />
						</Canvas>
						<TextBlock Text="{Binding Path=DataSeries.Title}" />
					</StackPanel>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="ToolTipTemplate">
			<Setter.Value>
				<ControlTemplate>
					<Border BorderBrush="LightGray" BorderThickness="1" CornerRadius="5" Background="White" Padding="3,1">
						<StackPanel Orientation="Horizontal">
							<TextBlock DataContext="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" Text="{Binding X, Converter={StaticResource IdentityValueConverter}}" />
							<TextBlock Text=", " />
							<TextBlock DataContext="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" Text="{Binding Y, Converter={StaticResource IdentityValueConverter}}" />
						</StackPanel>
					</Border>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="TrackballStyle">
			<Setter.Value>
				<Style TargetType="Polygon">
					<Setter Property="Fill" Value="Black" />
					<Setter Property="Stroke" Value="LightGray" />
				</Style>
			</Setter.Value>
		</Setter>
	</Style>
	<Style TargetType="charts:ColumnSeries" BasedOn="{StaticResource BarOrColumnSeriesBaseStyle}">
		<Setter Property="TrackballTemplate">
			<Setter.Value>
				<ControlTemplate TargetType="charts:Trackball">
					<primitives:ZoomCanvas x:Name="LayoutRoot">
						<Polygon Style="{TemplateBinding TrackballStyle}" Points="0,0 5,5 10,0 0,0" primitives:ZoomCanvas.ElementPosition="{Binding Path=RenderPoint, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource UIPointConverter}}">
							<Polygon.RenderTransform>
								<TranslateTransform X="-5" Y="-8" />
							</Polygon.RenderTransform>
						</Polygon>
					</primitives:ZoomCanvas>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style TargetType="charts:BarSeries" BasedOn="{StaticResource BarOrColumnSeriesBaseStyle}">
		<Setter Property="TrackballTemplate">
			<Setter.Value>
				<ControlTemplate TargetType="charts:Trackball">
					<primitives:ZoomCanvas x:Name="LayoutRoot">
						<Polygon Style="{TemplateBinding TrackballStyle}" Points="0,5 5,0 5,10 0,5" primitives:ZoomCanvas.ElementPosition="{Binding Path=RenderPoint, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource UIPointConverter}}">
							<Polygon.RenderTransform>
								<TranslateTransform X="3" Y="-5" />
							</Polygon.RenderTransform>
						</Polygon>
					</primitives:ZoomCanvas>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style TargetType="charts:Crosshair">
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="charts:Crosshair">
					<Canvas Name="CrossHair">
						<Line Stroke="{TemplateBinding Foreground}" X1="{TemplateBinding X}" X2="{TemplateBinding X}" Y1="0" Y2="5000" />
						<Line Stroke="{TemplateBinding Foreground}" Y1="{TemplateBinding Y}" Y2="{TemplateBinding Y}" X1="0" X2="5000" />
						<Grid x:Name="CrosshairLabelGrid" Canvas.Top="{TemplateBinding Y}" Canvas.Left="{TemplateBinding X}" Margin="5,-35,0,0" Height="30" Background="#80FFFFFF">
							<Grid.ColumnDefinitions>
								<ColumnDefinition />
								<ColumnDefinition />
							</Grid.ColumnDefinitions>
							<Grid.RowDefinitions>
								<RowDefinition />
								<RowDefinition />
							</Grid.RowDefinitions>
							<TextBlock Text="X: " />
							<TextBlock Text="{TemplateBinding FormattedXValue}" Grid.Column="1" />
							<TextBlock Text="Y: " Grid.Row="1" />
							<TextBlock Text="{TemplateBinding FormattedYValue}" Grid.Column="1" Grid.Row="1" />
						</Grid>
					</Canvas>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style TargetType="charts:ZoomRectangle">
		<Setter Property="Foreground" Value="DarkBlue" />
		<Setter Property="Background" Value="LightBlue" />
		<Setter Property="NotZoomableForeground" Value="Black" />
		<Setter Property="NotZoomableBackground" Value="Red" />
		<Setter Property="Opacity" Value="0.5" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="charts:ZoomRectangle">
					<Border x:Name="Border" BorderThickness="1" />
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style TargetType="charts:RulerRectangle">
		<Setter Property="BorderStyle">
			<Setter.Value>
				<Style TargetType="Border">
					<Setter Property="Background" Value="#446de884" />
					<Setter Property="BorderBrush" Value="#ff6de884" />
					<Setter Property="BorderThickness" Value="2" />
				</Style>
			</Setter.Value>
		</Setter>
		<Setter Property="LabelBorderStyle">
			<Setter.Value>
				<Style TargetType="Border">
					<Setter Property="Background" Value="White" />
					<Setter Property="BorderThickness" Value="1" />
					<Setter Property="BorderBrush" Value="Black" />
				</Style>
			</Setter.Value>
		</Setter>
		<Setter Property="RangeLabelStyle">
			<Setter.Value>
				<Style TargetType="TextBlock">
					<Setter Property="Padding" Value="4" />
				</Style>
			</Setter.Value>
		</Setter>
		<Setter Property="SizeLabelStyle">
			<Setter.Value>
				<Style TargetType="TextBlock">
					<Setter Property="Padding" Value="10" />
				</Style>
			</Setter.Value>
		</Setter>
		<Setter Property="LineStyle">
			<Setter.Value>
				<Style TargetType="Line">
					<Setter Property="StrokeThickness" Value="2" />
					<Setter Property="Stroke" Value="#323232" />
				</Style>
			</Setter.Value>
		</Setter>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="charts:RulerRectangle">
					<!-- This is the drawn rectangle and the height/width lines-->
					<Border x:Name="LayoutRoot" Style="{TemplateBinding BorderStyle}" Canvas.ZIndex="1">
						<Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Width="auto" Height="auto">
							<Canvas Name="RangeLabels" Canvas.ZIndex="4">
								<!-- These are the labels that are placed programmatically-->
								<Border x:Name="TopHolder" Style="{TemplateBinding LabelBorderStyle}" Visibility="Collapsed">
									<TextBlock x:Name="TopText" Style="{TemplateBinding RangeLabelStyle}" />
								</Border>
								<Border x:Name="BottomHolder" Style="{TemplateBinding LabelBorderStyle}" Visibility="Collapsed">
									<TextBlock x:Name="BottomText" Style="{TemplateBinding RangeLabelStyle}" />
								</Border>
								<Border x:Name="LeftHolder" Style="{TemplateBinding LabelBorderStyle}" Visibility="Collapsed">
									<TextBlock x:Name="LeftText" Style="{TemplateBinding RangeLabelStyle}" />
								</Border>
								<Border x:Name="RightHolder" Style="{TemplateBinding LabelBorderStyle}" Visibility="Collapsed">
									<TextBlock x:Name="RightText" Style="{TemplateBinding RangeLabelStyle}" />
								</Border>
							</Canvas>
							<!--panels to detect dragging-->
							<Grid Background="Transparent">
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="10" />
									<ColumnDefinition Width="*" />
									<ColumnDefinition Width="10" />
								</Grid.ColumnDefinitions>
								<Grid.RowDefinitions>
									<RowDefinition Height="10" />
									<RowDefinition Height="*" />
									<RowDefinition Height="10" />
								</Grid.RowDefinitions>
								<Canvas Name="LeftDragHandle" Grid.Row="1" VerticalAlignment="Stretch" Margin="-5 0 0 0" Width="10" Background="Transparent" Cursor="SizeWE" />
								<Canvas Name="TopDragHandle" Grid.Column="1" HorizontalAlignment="Stretch" Margin="0 -5 0 0" Height="10" Background="Transparent" Cursor="SizeNS" />
								<Canvas Name="RightDragHandle" Grid.Column="2" Grid.Row="1" VerticalAlignment="Stretch" Margin="0 0 -5 0" Width="10" Background="Transparent" Cursor="SizeWE" />
								<Canvas Name="BottomDragHandle" Grid.Column="1" Grid.Row="2" HorizontalAlignment="Stretch" Margin="0 0 0 -5" Height="10" Background="Transparent" Cursor="SizeNS" />
							</Grid>
							<Canvas primitives:Clip.ToBounds="True" x:Name="LineCanvas" Margin="0">
								<Line x:Name="WidthLine" Style="{TemplateBinding LineStyle}" />
								<Line x:Name="HeightLine" Style="{TemplateBinding LineStyle}" />
								<Border x:Name="WidthHolder" VerticalAlignment="Center" HorizontalAlignment="Center" Canvas.ZIndex="2">
									<TextBlock x:Name="WidthText" Style="{TemplateBinding SizeLabelStyle}" VerticalAlignment="Center" HorizontalAlignment="Center" />
								</Border>
								<Border x:Name="HeightHolder" VerticalAlignment="Center" HorizontalAlignment="Center" Canvas.ZIndex="2">
									<Border.RenderTransform>
										<RotateTransform Angle="270" />
									</Border.RenderTransform>
									<TextBlock x:Name="HeightText" Style="{TemplateBinding SizeLabelStyle}" />
								</Border>
							</Canvas>
						</Grid>
					</Border>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style TargetType="charts:Trackball">
		<Setter Property="TrackballStyle">
			<Setter.Value>
				<Style TargetType="Ellipse">
					<Setter Property="Fill" Value="Black" />
					<Setter Property="Width" Value="{StaticResource DefaultTrackballPointWidth}" />
					<Setter Property="Height" Value="{StaticResource DefaultTrackballPointHeight}" />
					<Setter Property="RenderTransform" Value="{StaticResource DefaultTrackballPointRenderTransform}" />
				</Style>
			</Setter.Value>
		</Setter>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="charts:Trackball">
					<primitives:ZoomCanvas x:Name="LayoutRoot">
						<Ellipse Style="{TemplateBinding TrackballStyle}" primitives:ZoomCanvas.ElementPosition="{Binding Path=RenderPoint, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource UIPointConverter}}" />
					</primitives:ZoomCanvas>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style TargetType="primitives:LegendLayoutContainer">
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="primitives:LegendLayoutContainer">
					<Grid Name="LayoutRoot">
						<Grid.ColumnDefinitions>
							<ColumnDefinition Width="Auto" />
							<ColumnDefinition Width="Auto" />
							<!-- axis -->
							<ColumnDefinition Width="Auto" />
							<!-- chart -->
							<ColumnDefinition />
							<ColumnDefinition Width="Auto" />
							<ColumnDefinition Width="Auto" />
							<!-- axis -->
							<ColumnDefinition Width="Auto" />
						</Grid.ColumnDefinitions>
						<Grid.RowDefinitions>
							<RowDefinition Height="Auto" />
							<RowDefinition Height="Auto" />
							<!-- axis -->
							<RowDefinition Height="Auto" />
							<!-- chart -->
							<RowDefinition />
							<RowDefinition Height="Auto" />
							<RowDefinition Height="Auto" />
							<!-- axis -->
							<RowDefinition Height="Auto" />
						</Grid.RowDefinitions>
						<ContentPresenter Content="{TemplateBinding ChartContent}" Grid.Row="2" Grid.RowSpan="3" Grid.Column="2" Grid.ColumnSpan="3" />
						<ContentPresenter Content="{TemplateBinding XAxisSecondaryContainer}" Grid.Row="1" Grid.Column="2" Grid.ColumnSpan="3" />
						<ContentPresenter Content="{TemplateBinding XAxisPrimaryContainer}" Grid.Row="5" Grid.Column="2" Grid.ColumnSpan="3" />
						<ContentPresenter Content="{TemplateBinding YAxisSecondaryContainer}" Grid.Row="2" Grid.RowSpan="3" Grid.Column="5" />
						<ContentPresenter Content="{TemplateBinding YAxisPrimaryContainer}" Grid.Row="2" Grid.RowSpan="3" Grid.Column="1" />
						<Grid x:Name="LegendOutsideTopContainer" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="7" />
						<Grid x:Name="LegendOutsideBottomContainer" Grid.Row="6" Grid.Column="0" Grid.ColumnSpan="7" />
						<Grid x:Name="LegendOutsideLeftContainer" Grid.Row="0" Grid.Column="0" Grid.RowSpan="7" />
						<Grid x:Name="LegendOutsideRightContainer" Grid.Row="0" Grid.Column="6" Grid.RowSpan="7">
							<ContentPresenter Content="{TemplateBinding LegendContent}" x:Name="LegendContentPresenter" />
						</Grid>
						<Grid x:Name="LegendInsideTopContainer" Grid.Row="2" Grid.Column="2" Grid.ColumnSpan="3" />
						<Grid x:Name="LegendInsideBottomContainer" Grid.Row="4" Grid.Column="2" Grid.ColumnSpan="3" />
						<Grid x:Name="LegendInsideLeftContainer" Grid.Row="2" Grid.Column="2" Grid.RowSpan="3" />
						<Grid x:Name="LegendInsideRightContainer" Grid.Row="2" Grid.Column="4" Grid.RowSpan="3" />
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style TargetType="charts:Legend">
		<Setter Property="TitleStyle">
			<Setter.Value>
				<Style TargetType="TextBlock">
					<Setter Property="HorizontalAlignment" Value="Center" />
					<Setter Property="FontSize" Value="12" />
					<Setter Property="FontWeight" Value="Bold" />
				</Style>
			</Setter.Value>
		</Setter>
		<Setter Property="Margin" Value="10" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="charts:Legend">
					<StackPanel>
						<ScrollViewer Style="{StaticResource ScrollViewerStyle}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" VerticalScrollBarVisibility="Auto" Background="{TemplateBinding Background}" Padding="10">
							<StackPanel Orientation="Vertical">
								<TextBlock Text="{TemplateBinding Title}" Style="{TemplateBinding TitleStyle}" Name="LegendTitle" />
								<StackPanel Orientation="Vertical" x:Name="LegendPanel" />
							</StackPanel>
						</ScrollViewer>
					</StackPanel>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="BorderBrush" Value="LightGray" />
		<Setter Property="Background" Value="Transparent" />
	</Style>
	<Style TargetType="charts:LegendItem">
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="Template">
			<Setter.Value>
				<!-- the default legend item template. A series may specify a different template
                in order to render the legend item in a different fashion -->
				<ControlTemplate TargetType="charts:LegendItem">
					<Grid HorizontalAlignment="Stretch" Margin="3" Background="{TemplateBinding Background}">
						<Grid.ColumnDefinitions>
							<ColumnDefinition />
							<ColumnDefinition />
						</Grid.ColumnDefinitions>
						<Rectangle Width="10" Height="10" />
						<TextBlock Text="{Binding Path=DataSeries.Title}" Grid.Column="1" />
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style TargetType="primitives:ZoomCanvas">
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="primitives:ZoomCanvas">
					<Canvas Name="LayoutRoot" />
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style TargetType="charts:AxisLabel" x:Key="AxisLabelBaseStyle">
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="charts:AxisLabel">
					<Grid x:Name="LayoutRoot" Width="{TemplateBinding CalculatedWidth}" Height="{TemplateBinding CalculatedHeight}">
						<Border x:Name="TextBorder" Padding="{TemplateBinding BorderPadding}" HorizontalAlignment="{TemplateBinding HorizontalAlignment}" VerticalAlignment="{TemplateBinding VerticalAlignment}" Style="{TemplateBinding BorderStyle}">
							<TextBlock x:Name="TextLabel" TextAlignment="{Binding RelativeSource={RelativeSource TemplatedParent},Path=HorizontalAlignment,Converter={StaticResource AlignmentConverter}}" Text="{TemplateBinding ActualText}" Style="{TemplateBinding TextStyle}" />
						</Border>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style TargetType="charts:AxisLabel" BasedOn="{StaticResource AxisLabelBaseStyle}" />
	<!-- ChartMultipleSeriesBase needs a reference to Palette -->
	<Style TargetType="charts:ChartMultipleSeriesBase" x:Key="ChartMultipleSeriesBaseStyle" BasedOn="{StaticResource ChartSeriesBaseStyle}">
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="charts:ChartMultipleSeriesBase">
					<Grid>
						<primitives:ZoomCanvas x:Name="LayoutRoot" />
						<Grid x:Name="SeriesContainer" />
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style TargetType="charts:ChartMultipleSeriesBase" BasedOn="{StaticResource ChartMultipleSeriesBaseStyle}" />
	<Style TargetType="charts:StackedColumnSeries" BasedOn="{StaticResource ChartMultipleSeriesBaseStyle}" />
	<Style TargetType="charts:StackedBarSeries" BasedOn="{StaticResource ChartMultipleSeriesBaseStyle}" />
	<Style TargetType="charts:StackedLineSeries" BasedOn="{StaticResource ChartMultipleSeriesBaseStyle}" />
</ResourceDictionary>