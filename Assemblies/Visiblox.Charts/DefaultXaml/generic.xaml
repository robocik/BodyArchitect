		</Setter>
	</Style>
	<Style TargetType="charts:LineSeries" BasedOn="{StaticResource LineSeriesStyle}" />
	<Style TargetType="charts:BoxPlotSeries" BasedOn="{StaticResource BoxPlotSeriesBaseStyle}" />
	<Style TargetType="charts:ErrorBarSeries" BasedOn="{StaticResource ErrorBarSeriesStyle}" />
	<Style TargetType="charts:DateTimeAxis">
		<Setter Property="HorizontalAlignment" Value="Stretch" />
		<Setter Property="VerticalAlignment" Value="Stretch" />
		<Setter Property="TitleStyle" Value="{StaticResource DefaultTitleStyle}" />
		<Setter Property="MajorGridlineStyle" Value="{StaticResource DefaultGridlineStyle}" />
		<Setter Property="MinorGridlineStyle" Value="{StaticResource DefaultGridlineStyle}" />
		<Setter Property="GridStripeStyle" Value="{StaticResource DefaultGridStripeStyle}" />
		<Setter Property="MajorTickStyle" Value="{StaticResource DefaultMajorTickStyle}" />
		<Setter Property="MinorTickStyle" Value="{StaticResource DefaultMinorTickStyle}" />
		<Setter Property="LabelStyle" Value="{StaticResource DefaultDateTimeLabelStyle}" />
		<Setter Property="AxisLineStyle" Value="{StaticResource DefaultAxisLineStyle}" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="charts:DateTimeAxis">
					<Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
						<Grid x:Name="LayoutRoot" Background="{TemplateBinding Background}">
							<!-- the grid rows / columns are created in response to the Orientation property value-->
							<Grid x:Name="LabelsContainer" />
							<primitives:TransposePanel x:Name="AxisLabelContainer">
								<TextBlock x:Name="AxisLabel" Text="{TemplateBinding Title}" Style="{TemplateBinding TitleStyle}" />
