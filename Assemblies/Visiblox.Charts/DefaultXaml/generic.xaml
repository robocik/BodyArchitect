<ResourceDictionary xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:system="clr-namespace:System;assembly=mscorlib" xmlns:primitives="clr-namespace:Visiblox.Charts.Primitives;assembly=Visiblox.Charts" xmlns:charts="clr-namespace:Visiblox.Charts;assembly=Visiblox.Charts" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
	<system:String x:Key="PaletteColorRed">#FFCF3B37</system:String>
	<system:String x:Key="PaletteColorBlack">#FF0C0F0F</system:String>
	<system:String x:Key="PaletteColorGreen">#FF60B167</system:String>
	<system:String x:Key="PaletteColorBlue">#FF436AA9</system:String>
	<system:String x:Key="PaletteColorOrange">#FFDA843F</system:String>
	<system:String x:Key="PaletteColorPurple">#FF6C4A98</system:String>
	<system:String x:Key="PaletteColorMaroon">#FF902F2D</system:String>
	<system:String x:Key="PaletteColorYellow">#FFF1E046</system:String>
	<Color x:Key="PaletteColorRedColor">#FFCF3B37</Color>
	<Color x:Key="PaletteColorBlackColor">#FF0C0F0F</Color>
	<Color x:Key="PaletteColorGreenColor">#FF60B167</Color>
	<Color x:Key="PaletteColorBlueColor">#FF436AA9</Color>
	<Color x:Key="PaletteColorOrangeColor">#FFDA843F</Color>
	<Color x:Key="PaletteColorPurpleColor">#FF6C4A98</Color>
	<Color x:Key="PaletteColorMaroonColor">#FF902F2D</Color>
	<Color x:Key="PaletteColorYellowColor">#FFF1E046</Color>
	<SolidColorBrush x:Key="PaletteColorRedBrush" Color="{StaticResource PaletteColorRedColor}" />
	<SolidColorBrush x:Key="PaletteColorBlackBrush" Color="{StaticResource PaletteColorBlackColor}" />
	<SolidColorBrush x:Key="PaletteColorGreenBrush" Color="{StaticResource PaletteColorGreenColor}" />
	<SolidColorBrush x:Key="PaletteColorBlueBrush" Color="{StaticResource PaletteColorBlueColor}" />
	<SolidColorBrush x:Key="PaletteColorOrangeBrush" Color="{StaticResource PaletteColorOrangeColor}" />
	<SolidColorBrush x:Key="PaletteColorPurpleBrush" Color="{StaticResource PaletteColorPurpleColor}" />
	<SolidColorBrush x:Key="PaletteColorMaroonBrush" Color="{StaticResource PaletteColorMaroonColor}" />
	<SolidColorBrush x:Key="PaletteColorYellowBrush" Color="{StaticResource PaletteColorYellowColor}" />
	<SolidColorBrush x:Key="DefaultNonPaletteColor" Color="{StaticResource PaletteColorRedColor}" />
	<SolidColorBrush x:Key="DefaultTransparentColor" Color="Transparent" />
	<system:Double x:Key="DefaultLineStrokeThickness">1</system:Double>
	<system:Double x:Key="DefaultSelectedLineStrokeThickness">2</system:Double>
	<system:Double x:Key="DefaultHighlightedLineStrokeThickness">3</system:Double>
	<system:Double x:Key="DefaultPointSize">5</system:Double>
	<system:Double x:Key="DefaultRadialPointSize">10</system:Double>
	<system:Double x:Key="DefaultPieExplode">0</system:Double>
	<system:Double x:Key="DefaultHighlightedPieExplode">0</system:Double>
	<system:Double x:Key="DefaultSelectedPieExplode">0.15</system:Double>
	<system:Double x:Key="DefaultBarOrColumnThickness">1</system:Double>
	<system:Double x:Key="DefaultTrackballPointWidth">4</system:Double>
	<system:Double x:Key="DefaultTrackballPointHeight">4</system:Double>
	<TranslateTransform x:Key="DefaultTrackballPointRenderTransform" X="-2" Y="-2" />
	<primitives:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
	<primitives:IdentityValueConverter x:Key="IdentityValueConverter" />
	<primitives:DataPointYValuesConverter x:Key="DataPointYValuesConverter" />
	<primitives:UIPointConverter x:Key="UIPointConverter" />
	<primitives:BoolToSignConverter x:Key="BoolToSignConverter" />
	<primitives:AlignmentConverter x:Key="AlignmentConverter" />
	<!-- default data label styles-->
	<Style x:Key="DefaultDataLabelBorderStyle" TargetType="Border">
		<Setter Property="BorderBrush" Value="Black" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="Margin" Value="0" />
		<Setter Property="Padding" Value="2" />
		<Setter Property="HorizontalAlignment" Value="Center" />
		<Setter Property="VerticalAlignment" Value="Center" />
		<Setter Property="Opacity" Value="0.6" />
		<Setter Property="Background" Value="White" />
		<Setter Property="CornerRadius" Value="2" />
	</Style>
	<Style x:Key="DefaultDataLabelTextStyle" TargetType="TextBlock">
		<Setter Property="Foreground" Value="Black" />
		<Setter Property="Opacity" Value="1.0" />
		<Setter Property="TextAlignment" Value="Left" />
		<Setter Property="HorizontalAlignment" Value="Left" />
		<Setter Property="VerticalAlignment" Value="Top" />
	</Style>
	<Style x:Key="DefaultDataLabelStyle" TargetType="charts:DataLabel">
		<Setter Property="BorderStyle" Value="{StaticResource DefaultDataLabelBorderStyle}" />
		<Setter Property="TextStyle" Value="{StaticResource DefaultDataLabelTextStyle}" />
	</Style>
	<!-- Default Axis styles -->
	<Style x:Key="DefaultTitleStyle" TargetType="TextBlock">
		<Setter Property="FontSize" Value="10" />
		<Setter Property="TextAlignment" Value="Center" />
	</Style>
	<Style x:Key="DefaultGridlineStyle" TargetType="Line">
		<Setter Property="Stroke" Value="#FFDEDEDE" />
		<Setter Property="StrokeThickness" Value="1" />
	</Style>
	<Style x:Key="DefaultGridStripeStyle" TargetType="Path">
		<Setter Property="Opacity" Value="0.15" />
		<Setter Property="Fill" Value="LightSeaGreen" />
	</Style>
	<Style x:Key="DefaultMajorTickStyle" TargetType="Line">
		<Setter Property="Stroke" Value="Black" />
		<Setter Property="StrokeThickness" Value="1" />
	</Style>
	<Style x:Key="DefaultMinorTickStyle" TargetType="Line">
		<Setter Property="Stroke" Value="Black" />
		<Setter Property="StrokeThickness" Value="1" />
	</Style>
	<Style x:Key="DefaultLabelStyle" TargetType="charts:AxisLabel">
		<Setter Property="TextStyle">
			<Setter.Value>
				<Style TargetType="TextBlock">
					<Setter Property="TextWrapping" Value="Wrap" />
				</Style>
			</Setter.Value>
		</Setter>
	</Style>
	<Style x:Key="DefaultDateTimeLabelStyle" TargetType="charts:AxisLabel">
		<Setter Property="TextStyle">
			<Setter.Value>
				<Style TargetType="TextBlock">
					<Setter Property="Margin" Value="3,0,0,0" />
					<Setter Property="TextAlignment" Value="Center" />
					<Setter Property="VerticalAlignment" Value="Top" />
					<Setter Property="TextWrapping" Value="Wrap" />
				</Style>
			</Setter.Value>
		</Setter>
	</Style>
	<Style x:Key="DefaultAxisLineStyle" TargetType="Line">
		<Setter Property="Stroke" Value="DarkGray" />
		<Setter Property="StrokeThickness" Value="1" />
	</Style>
	<!-- Radial Charts-->
	<Style x:Key="DefaultRadialAxisTitleStyle" TargetType="TextBlock">
		<Setter Property="FontSize" Value="12" />
		<Setter Property="FontWeight" Value="Bold" />
	</Style>
	<Style x:Key="DefaultRadialMajorGridlineStyle" TargetType="Path">
		<Setter Property="Stroke" Value="LightGray" />
		<Setter Property="StrokeThickness" Value="1" />
	</Style>
	<Style x:Key="DefaultRadialMinorGridlineStyle" TargetType="Path">
		<Setter Property="Stroke" Value="LightGray" />
		<Setter Property="StrokeThickness" Value="1" />
	</Style>
	<Style x:Key="DefaultRadialLabelLineStyleLineStyle" TargetType="Path">
		<Setter Property="Stroke" Value="Gray" />
		<Setter Property="StrokeThickness" Value="1" />
		<Setter Property="StrokeDashArray" Value="5,5" />
	</Style>
	<!-- Styles below here to be pulled in by both version-specific and common -->
	<Style TargetType="charts:ChartSeriesCommonBase" x:Key="ChartSeriesBaseStyle">
		<Setter Property="HorizontalAlignment" Value="Stretch" />
		<Setter Property="VerticalAlignment" Value="Stretch" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="charts:ChartSeriesCommonBase">
					<primitives:ZoomCanvas x:Name="LayoutRoot" />
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="TrackballTemplate">
			<Setter.Value>
				<ControlTemplate TargetType="charts:Trackball">
					<primitives:ZoomCanvas x:Name="LayoutRoot">
						<Ellipse Style="{TemplateBinding TrackballStyle}" primitives:ZoomCanvas.ElementPosition="{Binding Path=RenderPoint, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource UIPointConverter}}" />
					</primitives:ZoomCanvas>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="TrackballStyle">
			<Setter.Value>
				<Style TargetType="Ellipse">
					<Setter Property="Fill" Value="Black" />
					<Setter Property="Width" Value="{StaticResource DefaultTrackballPointWidth}" />
					<Setter Property="Height" Value="{StaticResource DefaultTrackballPointHeight}" />
					<Setter Property="RenderTransform" Value="{StaticResource DefaultTrackballPointRenderTransform}" />
				</Style>
			</Setter.Value>
		</Setter>
		<Setter Property="ToolTipTemplate">
			<Setter.Value>
				<ControlTemplate>
					<Border BorderBrush="LightGray" BorderThickness="1" CornerRadius="5" Background="White" Padding="3,1">
						<StackPanel Orientation="Horizontal">
							<TextBlock DataContext="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" Text="{Binding X, Converter={StaticResource IdentityValueConverter}}" />
							<TextBlock Text=", " />
							<TextBlock DataContext="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" Text="{Binding Y, Converter={StaticResource IdentityValueConverter}}" />
						</StackPanel>
					</Border>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style TargetType="charts:ChartSeriesCommonBase" BasedOn="{StaticResource ChartSeriesBaseStyle}" />
	<Style TargetType="charts:LineSeries" BasedOn="{StaticResource ChartSeriesBaseStyle}" x:Key="LineSeriesStyle">
		<Setter Property="PointFill" Value="{StaticResource DefaultNonPaletteColor}" />
		<Setter Property="PointStroke" Value="{StaticResource DefaultTransparentColor}" />
		<Setter Property="PointStrokeThickness" Value="{StaticResource DefaultLineStrokeThickness}" />
		<Setter Property="PointSize" Value="{StaticResource DefaultPointSize}" />
		<Setter Property="LineStroke" Value="{StaticResource DefaultNonPaletteColor}" />
		<Setter Property="LineStrokeThickness" Value="{StaticResource DefaultLineStrokeThickness}" />
		<Setter Property="AreaFill" Value="{StaticResource DefaultNonPaletteColor}" />
		<Setter Property="LabelStyle" Value="{StaticResource DefaultDataLabelStyle}" />
		<Setter Property="LegendItemTemplate">
			<Setter.Value>
				<ControlTemplate TargetType="charts:LegendItem">
					<StackPanel Orientation="Horizontal" HorizontalAlignment="Left">
						<Canvas Width="10" Height="10" Margin="0 0 9 0" Opacity="{Binding Opacity}">
							<Path Data="M 0,5 L 10,5 L 10,10 L 0,10" Fill="{Binding AreaFill}" Style="{Binding Path=AreaStyle}" Visibility="{Binding Path=ShowArea, Converter={StaticResource BoolToVisibilityConverter}}" />
							<Path Data="M 0,5 L 10,5" Stroke="{Binding LineStroke}" Style="{Binding Path=LineStyle}" Visibility="{Binding Path=ShowLine, Converter={StaticResource BoolToVisibilityConverter}}" />
							<Ellipse Width="4" Height="4" Fill="{Binding PointFill}" Style="{Binding Path=PointStyle}" Visibility="{Binding Path=ShowPoints, Converter={StaticResource BoolToVisibilityConverter}}">
								<Ellipse.RenderTransform>
									<TranslateTransform X="3" Y="3" />
								</Ellipse.RenderTransform>
							</Ellipse>
						</Canvas>
						<TextBlock Text="{Binding Path=DataSeries.Title}" />
					</StackPanel>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style TargetType="charts:BandSeries" x:Key="BandSeriesStyle" BasedOn="{StaticResource ChartSeriesBaseStyle}">
		<Setter Property="UpperLineStroke" Value="{StaticResource DefaultNonPaletteColor}" />
		<Setter Property="LowerLineStroke" Value="{StaticResource DefaultNonPaletteColor}" />
		<Setter Property="UpperLineStrokeThickness" Value="{StaticResource DefaultLineStrokeThickness}" />
		<Setter Property="LowerLineStrokeThickness" Value="{StaticResource DefaultLineStrokeThickness}" />
		<Setter Property="AreaStyle">
			<Setter.Value>
				<Style TargetType="FrameworkElement">
					<Setter Property="Opacity" Value="0.7" />
				</Style>
			</Setter.Value>
		</Setter>
		<Setter Property="InvertedAreaStyle">
			<Setter.Value>
				<Style TargetType="FrameworkElement">
					<Setter Property="Opacity" Value="0.7" />
				</Style>
			</Setter.Value>
		</Setter>
		<Setter Property="LegendItemTemplate">
			<Setter.Value>
				<ControlTemplate TargetType="charts:LegendItem">
					<StackPanel Orientation="Horizontal" HorizontalAlignment="Left">
						<Canvas Width="10" Height="10" Margin="0 0 9 0" Opacity="{Binding Opacity}">
							<Path Data="M 0,0 L 10,10" Stroke="{Binding UpperLineStroke}" Style="{Binding Path=UpperLineStyle}" Visibility="{Binding Path=ShowLines, Converter={StaticResource BoolToVisibilityConverter}}" />
							<Path Data="M 0,0 L 5,5 L 0,10" Opacity="{Binding Path=AreaFill.Opacity}" Fill="{Binding AreaFill}" Style="{Binding Path=AreaStyle}" Visibility="{Binding Path=ShowArea, Converter={StaticResource BoolToVisibilityConverter}}" />
							<Path Data="M 10,0 L 5,5 L 10,10" Opacity="{Binding Path=AreaFill.Opacity}" Fill="{Binding InvertedAreaFill}" Style="{Binding Path=InvertedAreaStyle}" Visibility="{Binding Path=ShowArea, Converter={StaticResource BoolToVisibilityConverter}}" />
							<Path Data="M 0,10 L 10,0" Stroke="{Binding LowerLineStroke}" Style="{Binding Path=LowerLineStyle}" Visibility="{Binding Path=ShowLines, Converter={StaticResource BoolToVisibilityConverter}}" />
						</Canvas>
						<TextBlock Text="{Binding Path=DataSeries.Title}" />
					</StackPanel>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="TrackballTemplate">
			<Setter.Value>
				<ControlTemplate TargetType="charts:Trackball">
					<primitives:ZoomCanvas x:Name="LayoutRoot">
						<Ellipse Style="{TemplateBinding TrackballStyle}" primitives:ZoomCanvas.ElementPosition="{Binding Path=RenderPoint, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource UIPointConverter}, ConverterParameter=Upper}" />
						<Ellipse Style="{TemplateBinding TrackballStyle}" primitives:ZoomCanvas.ElementPosition="{Binding Path=RenderPoint, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource UIPointConverter}, ConverterParameter=Lower}" />
					</primitives:ZoomCanvas>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="TrackballStyle">
			<Setter.Value>
				<Style TargetType="Ellipse">
					<Setter Property="Fill" Value="Black" />
					<Setter Property="Width" Value="{StaticResource DefaultTrackballPointWidth}" />
					<Setter Property="Height" Value="{StaticResource DefaultTrackballPointHeight}" />
					<Setter Property="RenderTransform" Value="{StaticResource DefaultTrackballPointRenderTransform}" />
				</Style>
			</Setter.Value>
		</Setter>
	</Style>
	<Style TargetType="charts:ErrorBarSeries" x:Key="ErrorBarSeriesStyle" BasedOn="{StaticResource ChartSeriesBaseStyle}">
		<Setter Property="LineStroke" Value="{StaticResource DefaultNonPaletteColor}" />
		<Setter Property="LineStrokeThickness" Value="{StaticResource DefaultLineStrokeThickness}" />
		<Setter Property="LineStyle">
			<Setter.Value>
				<Style TargetType="Path">
					<Setter Property="StrokeLineJoin" Value="Bevel" />
				</Style>
			</Setter.Value>
		</Setter>
		<Setter Property="SelectedStyle">
			<Setter.Value>
				<Style TargetType="charts:ErrorBarSeries">
					<Setter Property="LineStrokeThickness" Value="{StaticResource DefaultSelectedLineStrokeThickness}" />
					<Setter Property="LineStyle">
						<Setter.Value>
							<Style TargetType="Path">
								<Setter Property="StrokeLineJoin" Value="Miter" />
								<Setter Property="StrokeMiterLimit" Value="1" />
							</Style>
						</Setter.Value>
					</Setter>
				</Style>
			</Setter.Value>
		</Setter>
		<Setter Property="LegendItemTemplate">
			<Setter.Value>
				<ControlTemplate TargetType="charts:LegendItem">
					<StackPanel Orientation="Horizontal" HorizontalAlignment="Left">
						<Canvas Width="10" Height="10" Margin="0 0 9 0" Opacity="{Binding Opacity}">
							<Path Data="M 0,5 L 10,5" Stroke="{Binding LineStroke}" Style="{Binding Path=LineStyle}" />
						</Canvas>
						<TextBlock Text="{Binding Path=DataSeries.Title}" />
					</StackPanel>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="ToolTipTemplate">
			<Setter.Value>
				<ControlTemplate>
					<Border BorderBrush="LightGray" BorderThickness="1" CornerRadius="5" Background="White" Padding="3,1">
						<Grid>
							<Grid.RowDefinitions>
								<RowDefinition />
								<RowDefinition />
								<RowDefinition />
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition />
								<ColumnDefinition />
							</Grid.ColumnDefinitions>
							<TextBlock Grid.Row="0" Text="X:" />
							<TextBlock Grid.Row="0" Grid.Column="1" DataContext="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" Text="{Binding X, Converter={StaticResource IdentityValueConverter}}" />
							<TextBlock Grid.Row="1" Text="Minimum:" />
							<TextBlock Grid.Row="1" Grid.Column="1" DataContext="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" Text="{Binding Converter={StaticResource DataPointYValuesConverter}, ConverterParameter=Minimum}" />
							<TextBlock Grid.Row="2" Text="Maximum:" />
							<TextBlock Grid.Row="2" Grid.Column="1" DataContext="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" Text="{Binding Converter={StaticResource DataPointYValuesConverter}, ConverterParameter=Maximum}" />
						</Grid>
					</Border>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="TrackballTemplate">
			<Setter.Value>
				<ControlTemplate TargetType="charts:Trackball">
					<primitives:ZoomCanvas x:Name="LayoutRoot">
						<Polygon Style="{TemplateBinding TrackballStyle}" Points="0,0 5,5 10,0 0,0" primitives:ZoomCanvas.ElementPosition="{Binding Path=RenderPoint, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource UIPointConverter}, ConverterParameter=Maximum}">
							<Polygon.RenderTransform>
								<TransformGroup>
									<TranslateTransform X="-5" Y="-8" />
									<ScaleTransform>
										<ScaleTransform.ScaleY>
											<Binding Path="Series.YAxis.ReverseAxis" RelativeSource="{RelativeSource TemplatedParent}" Converter="{StaticResource BoolToSignConverter}" ConverterParameter="1" />
										</ScaleTransform.ScaleY>
									</ScaleTransform>
								</TransformGroup>
							</Polygon.RenderTransform>
						</Polygon>
						<Polygon Style="{TemplateBinding TrackballStyle}" Points="0,5 5,0 10,5 0,5" primitives:ZoomCanvas.ElementPosition="{Binding Path=RenderPoint, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource UIPointConverter}, ConverterParameter=Minimum}">
							<Polygon.RenderTransform>
								<TransformGroup>
									<TranslateTransform X="-5" Y="3" />
									<ScaleTransform>
										<ScaleTransform.ScaleY>
											<Binding Path="Series.YAxis.ReverseAxis" RelativeSource="{RelativeSource TemplatedParent}" Converter="{StaticResource BoolToSignConverter}" ConverterParameter="1" />
										</ScaleTransform.ScaleY>
									</ScaleTransform>
								</TransformGroup>
							</Polygon.RenderTransform>
						</Polygon>
					</primitives:ZoomCanvas>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="TrackballStyle">
			<Setter.Value>
				<Style TargetType="Polygon">
					<Setter Property="Fill" Value="Black" />
					<Setter Property="Stroke" Value="LightGray" />
				</Style>
			</Setter.Value>
		</Setter>
	</Style>
	<Style TargetType="charts:BoxPlotSeries" x:Key="BoxPlotSeriesBaseStyle" BasedOn="{StaticResource ChartSeriesBaseStyle}">
		<Setter Property="PointFill" Value="{StaticResource DefaultNonPaletteColor}" />
		<Setter Property="PointStroke" Value="{StaticResource DefaultNonPaletteColor}" />
		<Setter Property="PointStrokeThickness" Value="{StaticResource DefaultLineStrokeThickness}" />
		<Setter Property="SelectedStyle">
			<Setter.Value>
				<Style TargetType="charts:BoxPlotSeries">
					<Setter Property="PointStrokeThickness" Value="{StaticResource DefaultSelectedLineStrokeThickness}" />
					<Setter Property="PointStyle">
						<Setter.Value>
							<Style TargetType="Path">
								<Setter Property="StrokeLineJoin" Value="Miter" />
								<Setter Property="StrokeMiterLimit" Value="1" />
							</Style>
						</Setter.Value>
					</Setter>
				</Style>
			</Setter.Value>
		</Setter>
		<Setter Property="LegendItemTemplate">
			<Setter.Value>
				<ControlTemplate TargetType="charts:LegendItem">
					<StackPanel Orientation="Horizontal" HorizontalAlignment="Left">
						<Canvas Width="10" Height="10" Margin="0 0 9 0" Opacity="{Binding Opacity}">
							<Path Data="M 0,5 L 10,5" Fill="{Binding PointFill}" Stroke="{Binding PointStroke}" Style="{Binding Path=PointStyle}" />
						</Canvas>
						<TextBlock Text="{Binding Path=DataSeries.Title}" />
					</StackPanel>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="ToolTipTemplate">
			<Setter.Value>
				<ControlTemplate>
					<Border BorderBrush="LightGray" BorderThickness="1" CornerRadius="5" Background="White" Padding="3,1">
						<Grid>
							<Grid.RowDefinitions>
								<RowDefinition />
								<RowDefinition />
								<RowDefinition />
								<RowDefinition />
								<RowDefinition />
								<RowDefinition />
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition />
								<ColumnDefinition />
							</Grid.ColumnDefinitions>
							<TextBlock Grid.Row="0" Text="X:" />
							<TextBlock Grid.Row="0" Grid.Column="1" DataContext="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" Text="{Binding X, Converter={StaticResource IdentityValueConverter}}" />
							<TextBlock Grid.Row="1" Text="Minimum:" />
							<TextBlock Grid.Row="1" Grid.Column="1" DataContext="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" Text="{Binding Converter={StaticResource DataPointYValuesConverter}, ConverterParameter=Minimum}" />
							<TextBlock Grid.Row="2" Text="Lower Quartile:" />
							<TextBlock Grid.Row="2" Grid.Column="1" DataContext="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" Text="{Binding Converter={StaticResource DataPointYValuesConverter}, ConverterParameter=LowerQuartile}" />
							<TextBlock Grid.Row="3" Text="Median:" />
							<TextBlock Grid.Row="3" Grid.Column="1" DataContext="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" Text="{Binding Converter={StaticResource DataPointYValuesConverter}, ConverterParameter=Median}" />
							<TextBlock Grid.Row="4" Text="Upper Quartile:" />
							<TextBlock Grid.Row="4" Grid.Column="1" DataContext="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" Text="{Binding Converter={StaticResource DataPointYValuesConverter}, ConverterParameter=UpperQuartile}" />
							<TextBlock Grid.Row="5" Text="Maximum:" />
							<TextBlock Grid.Row="5" Grid.Column="1" DataContext="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" Text="{Binding Converter={StaticResource DataPointYValuesConverter}, ConverterParameter=Maximum}" />
						</Grid>
					</Border>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="TrackballTemplate">
			<Setter.Value>
				<ControlTemplate TargetType="charts:Trackball">
					<primitives:ZoomCanvas x:Name="LayoutRoot">
						<Polygon Style="{TemplateBinding TrackballStyle}" Points="0,0 5,5 10,0 0,0" primitives:ZoomCanvas.ElementPosition="{Binding Path=RenderPoint, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource UIPointConverter}, ConverterParameter=Maximum}">
							<Polygon.RenderTransform>
								<TransformGroup>
									<TranslateTransform X="-5" Y="-8" />
									<ScaleTransform>
										<ScaleTransform.ScaleY>
											<Binding Path="Series.YAxis.ReverseAxis" RelativeSource="{RelativeSource TemplatedParent}" Converter="{StaticResource BoolToSignConverter}" ConverterParameter="1" />
										</ScaleTransform.ScaleY>
									</ScaleTransform>
								</TransformGroup>
							</Polygon.RenderTransform>
						</Polygon>
						<Polygon Style="{TemplateBinding TrackballStyle}" Points="0,5 5,0 10,5 0,5" primitives:ZoomCanvas.ElementPosition="{Binding Path=RenderPoint, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource UIPointConverter}, ConverterParameter=Minimum}">
							<Polygon.RenderTransform>
								<TransformGroup>
									<TranslateTransform X="-5" Y="3" />
									<ScaleTransform>
										<ScaleTransform.ScaleY>
											<Binding Path="Series.YAxis.ReverseAxis" RelativeSource="{RelativeSource TemplatedParent}" Converter="{StaticResource BoolToSignConverter}" ConverterParameter="1" />
										</ScaleTransform.ScaleY>
									</ScaleTransform>
								</TransformGroup>
							</Polygon.RenderTransform>
						</Polygon>
					</primitives:ZoomCanvas>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="TrackballStyle">
			<Setter.Value>
				<Style TargetType="Polygon">
					<Setter Property="Fill" Value="Black" />
					<Setter Property="Stroke" Value="LightGray" />
				</Style>
			</Setter.Value>
		</Setter>
	</Style>
	<ControlTemplate x:Key="PieChartTemplate" TargetType="charts:PieChart">
		<Grid x:Name="LayoutRoot" Background="{TemplateBinding Background}" Opacity="{TemplateBinding Opacity}">
			<Grid.ColumnDefinitions>
				<ColumnDefinition />
			</Grid.ColumnDefinitions>
			<Grid.RowDefinitions>
