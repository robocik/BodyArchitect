						<TextBlock Text="{Binding Path=DataPoint.X}" />
					</StackPanel>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style TargetType="charts:NumericAxisBase">
		<Setter Property="HorizontalAlignment" Value="Stretch" />
		<Setter Property="VerticalAlignment" Value="Stretch" />
		<Setter Property="TitleStyle" Value="{StaticResource DefaultTitleStyle}" />
		<Setter Property="MajorGridlineStyle" Value="{StaticResource DefaultGridlineStyle}" />
		<Setter Property="MinorGridlineStyle" Value="{StaticResource DefaultGridlineStyle}" />
		<Setter Property="GridStripeStyle" Value="{StaticResource DefaultGridStripeStyle}" />
		<Setter Property="MajorTickStyle" Value="{StaticResource DefaultMajorTickStyle}" />
		<Setter Property="MinorTickStyle" Value="{StaticResource DefaultMinorTickStyle}" />
		<Setter Property="LabelStyle" Value="{StaticResource DefaultLabelStyle}" />
		<Setter Property="AxisLineStyle" Value="{StaticResource DefaultAxisLineStyle}" />
		<Setter Property="Background" Value="Transparent" />
