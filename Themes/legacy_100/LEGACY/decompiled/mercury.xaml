            <Border Border.Name="SelectedBorder" UIElement.Opacity="0" Border.BorderThickness="1" Border.CornerRadius="2" Border.BorderBrush="{DynamicResource SelectedListItemBorderBrush}">
              <Border Border.BorderBrush="{DynamicResource ListItemSelectedInsiderBorderBrush}" Border.BorderThickness="1" Border.CornerRadius="1" Border.Background="{DynamicResource ListItemSelectedBackgroundBrush}" />
