            <Border Border.Name="Border" Border.BorderBrush="{DynamicResource ScrollBarButtonBorderBrush}" Border.BorderThickness="{TemplateBinding Control.BorderThickness}" Border.CornerRadius="1" Border.Background="{DynamicResource ScrollBarButtonBrush}" />
            <Border Border.Name="HoverBorder" Border.BorderBrush="{DynamicResource ScrollBarButtonBorderBrush}" Border.BorderThickness="{TemplateBinding Control.BorderThickness}" Border.CornerRadius="1" Border.Background="{DynamicResource ScrollBarButtonHoverBrush}" UIElement.Opacity="0" />
            <Border Border.Name="PressedBorder" Border.BorderBrush="{DynamicResource ScrollBarButtonBorderBrush}" Border.BorderThickness="{TemplateBinding Control.BorderThickness}" Border.CornerRadius="1" Border.Background="{DynamicResource ScrollBarPressedBrush}" UIElement.Opacity="0" />
