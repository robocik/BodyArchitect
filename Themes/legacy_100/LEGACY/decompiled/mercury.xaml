            <Border Border.Name="HoverBorder" Border.BorderThickness="1" Border.BorderBrush="{DynamicResource MouseOverBorderBrush}" Border.CornerRadius="3" UIElement.Opacity="0">
              <Border Border.BorderThickness="1" Border.CornerRadius="2" Border.Background="{DynamicResource ListItemHoverBrush}">
                <Border.BorderBrush>
                  <SolidColorBrush SolidColorBrush.Color="{DynamicResource WhiteColor}" />
                </Border.BorderBrush>
              </Border>
            </Border>
            <ContentPresenter FrameworkElement.HorizontalAlignment="{TemplateBinding Control.HorizontalContentAlignment}" FrameworkElement.VerticalAlignment="{TemplateBinding Control.VerticalContentAlignment}" FrameworkElement.Margin="10,4,0,4" />
          </Grid>
          <ControlTemplate.Triggers>
            <Trigger Trigger.Property="UIElement.IsMouseOver" Trigger.Value="True">
              <Setter Setter.TargetName="HoverBorder" Setter.Property="UIElement.Opacity" Setter.Value="1" />
            </Trigger>
            <Trigger Trigger.Property="ListBoxItem.IsSelected" Trigger.Value="True">
              <Setter Setter.Value="{DynamicResource SelectedBackgroundBrush}" Setter.TargetName="HoverBorder" Setter.Property="Border.Background" />
              <Setter Setter.TargetName="SelectedBorder" Setter.Property="UIElement.Opacity" Setter.Value="1" />
            </Trigger>
            <Trigger Trigger.Property="UIElement.IsEnabled" Trigger.Value="False">
              <Setter Setter.Value="{DynamicResource DisabledForegroundBrush}" Setter.Property="Control.Foreground" />
            </Trigger>
          </ControlTemplate.Triggers>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
